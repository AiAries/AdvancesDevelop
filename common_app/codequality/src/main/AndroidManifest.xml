    <application
        android:name=".base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".business.main_page.MainActivity" />

        <activity android:name=".business.games.GameActivity" />
        <activity android:name=".business.chat.ChatActivity" />
        <activity android:name=".business.enter.EnterListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".business.infinite_viewpage.InfiniteViewPageActivity" />
        <activity android:name=".business.databinding.BindDemoActivity" />
        <activity android:name=".business.webview.WebViewActivity" />
        <activity android:name=".business.custom_view.CustomViewActivity" />

    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>